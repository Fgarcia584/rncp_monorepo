# Frontend Environment Variables Example
# Copy this file to .env and fill in your actual values

# API URL for the backend service
VITE_API_URL=http://localhost:3001

# Frontend URL (used for CORS and redirects)
FRONTEND_URL=http://localhost:3000

# Google Maps API Key (get from Google Cloud Console)
# Make sure to enable Places API and Maps JavaScript API
VITE_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Sentry Configuration
# Get your DSN from https://sentry.io/settings/[your-org]/projects/[your-project]/keys/
VITE_SENTRY_DSN=
# Example: VITE_SENTRY_DSN=https://abc123@o123456.ingest.sentry.io/123456

# Environment (development, staging, production)
VITE_ENVIRONMENT=development

# Application Version (for release tracking)
VITE_APP_VERSION=1.0.0

# Sentry Build Configuration (for source maps upload)
# Required for production builds
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_AUTH_TOKEN=
